{
  "name": "@bedolla/enrivision",
  "version": "0.1.0",
  "description": "MCP server that uploads local media to EnriProxy and returns server-side analysis (resumable uploads).",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "enrivision",
    "enriproxy",
    "vision",
    "video",
    "audio",
    "image",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bedolla/EnriVision.git"
  },
  "bugs": {
    "url": "https://github.com/Bedolla/EnriVision/issues"
  },
  "homepage": "https://github.com/Bedolla/EnriVision#readme",
  "license": "AGPL-3.0-only",
  "author": "Pipochas",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "enrivision": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "build": "tsc -p tsconfig.json",
    "prepack": "npm run build",
    "start": "node dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.1",
    "mime-types": "3.0.2"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": ">=22"
  }
}
